{
  "name": "lun-cli",
  "version": "2.4.3",
  "description": "generator react component using cli",
  "main": "dist/index.cjs",
  "bin": {
    "lun-cli": "bin/luna.js"
  },
  "scripts": {
    "start": "node bin/luna.js",
    "build": "unbuild",
    "dev": "pnpm build && pnpm start",
    "changeversion": "node ./scripts/changeversion.js",
    "prepublish": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "exports": {
    "import": "./dist/lunConfig.mjs",
    "require": "./dist/lunConfig.cjs",
    "types": "./dist/index.d.ts",
    "default": "./dist/index.d.cts"
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "build.config.ts",
    "package.json",
    "tsconfig.json"
  ],
  "keywords": [
    "react",
    "typescript",
    "cac",
    "react-ts",
    "javascript",
    "cli"
  ],
  "author": {
    "name": "Juanestban"
  },
  "repository": "https://github.com/Juanestban/luna-cli",
  "license": "ISC",
  "dependencies": {
    "cac": "6.7.14",
    "minimist": "1.2.8",
    "picocolors": "1.0.0"
  },
  "bugs": "https://github.com/Juanestban/luna-cli/issues",
  "devDependencies": {
    "@types/minimist": "1.2.2",
    "@types/node": "22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "8.4.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.0.0",
    "eslint-module-utils": "2.8.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-promise": "^7.1.0",
    "prettier": "3.0.3",
    "typescript": "*",
    "unbuild": "2.0.0"
  }
}
